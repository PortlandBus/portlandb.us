<%= simple_form_for(@volunteer) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :address_1 %>
    <%= f.input :address_2 %>
    <%= f.input :email %>
    <%= f.input :phone %>
    <%= f.input :focus %>
    <%= f.input :essay %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
